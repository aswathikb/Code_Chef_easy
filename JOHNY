#include <bits/stdc++.h>
using namespace std;
 
int main() {
    int t,n,a[100],song,k,i;
    cin>>t;
    while(t--){
        int result=0;
        cin>>n;
        for(i=0;i<n;i++){
            cin>>a[i];
        }
        cin>>k;
        song=a[k-1];
        sort(a,a+n);
        for(i=0;i<n;i++){
            if(a[i]==song){
            result=i;
            cout<<result+1<<endl;
        }
        }
    }
	// your code goes here
	return 0;
}
