#include <bits/stdc++.h>
using namespace std;
 
int main() {
    int N,sum1;
	cin>>N;
	sum1=N*(N+1)/2;
	int a[N];
	for (int i = 0; i <N; ++i)
	{
		cin>>a[i];
	}
    int sum2=0;
    for(int i=0;i<N;i++)
    	sum2+=a[i];
    if(sum1==sum2)
    	cout<<"YES\n";
    else
    	cout<<"NO\n";
	// your code goes here
	return 0;
}
