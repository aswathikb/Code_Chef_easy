#include <bits/stdc++.h>
using namespace std;
 
int main() {
    int t,N;
    cin>>t;
    while(t--){
        int count=1;
        cin>>N;
        int n[N];
        for(int i=0;i<N;i++){
            cin>>n[i];
        }
        int m=n[0];
        for(int i=1;i<N;i++){
            if(m>n[i])
              {  m=n[i];
                count++;
            }
        }
        cout<<count<<endl;
    }
	// your code goes here
	return 0;
}
